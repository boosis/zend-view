{
	"name": "boosis/zend-view",
	"description": "Zend Framework 1 View package",
	"keywords": [
		"framework",
		"zf1",
		"zend",
		"view"
	],
	"homepage": "http://framework.zend.com/",
	"license": "BSD-3-Clause",
	"require": {
	  "boosis/zend-loader": "dev-master",
	  "zf1/zend-controller": "1.12.8",
		"zf1/zend-exception": "1.12.8",
		"zf1/zend-locale": "1.12.8",
		"zf1/zend-registry": "1.12.8"
	},
	"autoload": {
		"psr-0": {
			"Zend_View": "library/"
		}
	},
	"suggest": {
		"zf1/zend-json": "Used in special situations or with special adapters",
		"zf1/zend-layout": "Used in special situations or with special adapters"
	},
	 "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/boosis/zend-loader"
        }
    ]
}
